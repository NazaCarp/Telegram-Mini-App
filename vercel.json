{
  "version": 2,
  "name": "mini-app-ejemplo",
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/friends",
      "dest": "/friends.html"
    }
  ]
}